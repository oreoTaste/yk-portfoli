<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.portfoli.dao.QnaCategoryDao">

  <resultMap type="QnaCategory" id="QnaCategoryMap">
    <id column="category_no" property="number" />
    <result column="name" property="name" />
  </resultMap>

  <select id="findAll" resultMap="QnaCategoryMap">
    SELECT *
    FROM pf_question_category
  </select>
  


</mapper>







